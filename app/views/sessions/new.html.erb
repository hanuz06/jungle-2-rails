<div class="container_for_login_wrapper">
  <div class="login_signup_wrapper">

    <div>
      <h1>Login</h1>
    </div>

    <%= form_tag '/login' do %>

      <div class="container_style">
        Email:<br/> <%= text_field_tag :email %><br/><br/>
        Password:<br/> <%= password_field_tag :password %><br/><br/>
        <%= submit_tag "Submit" %>
      </div>
    <% end %>

  </div>
</div>


